<apex:page controller="VF_Controller">
    <apex:form >
    
    <apex:pageBlock >
        <apex:pageBlockButtons >
            <apex:commandButton action="{!callMeth}" value="Get Data"/>
        </apex:pageBlockButtons>
        <apex:pageBlockTable value="{!accList}" var="acc" rendered="{!flag}">
            <apex:column value="{!acc.id}"/>
            <apex:column value="{!acc.name}"/>
        </apex:pageBlockTable> 
    </apex:pageBlock>
</apex:form> 
</apex:page>